---
import featuredProjects from '../constants/index.ts';
import FeaturedProjectCard from './UI/FeaturedProjectCard.astro';
---

<section class="max-w-6xl mx-auto pb-6 pt-2 mb-12 my-6 px-6 xl:px-0 ">
    <h2 class="text-6xl mb-8 text-right"><span class="font-bold ">What</span> do I do</span></h2>
    <div class="grid gap-4 md:grid-cols-3 lg:grid-cols-5 md:gap-1 ">
    {featuredProjects.map(work => (
        <FeaturedProjectCard item={work} />
    ))}
    
    </div>
</section>